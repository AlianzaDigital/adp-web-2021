RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteCond %{REQUEST_URI} !^/\.well-known/cpanel-dcv/[0-9a-zA-Z_-]+$
RewriteCond %{REQUEST_URI} !^/\.well-known/pki-validation/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^(.*)$ https://www.altosdelpalmar.com/$1 [R,L]

Redirect 301 /prueba-c/casas-campestres-melgar.html https://www.altosdelpalmar.com/casas-campestres.html
Redirect 301 /casas-campestres-melgar.html https://www.altosdelpalmar.com/casas-campestres.html

Redirect 301 /casas-campestres https://www.altosdelpalmar.com/casas-campestres.html
Redirect 301 /condominio https://www.altosdelpalmar.com/condominio.html
Redirect 301 /ubicacion https://www.altosdelpalmar.com/ubicacion.html
Redirect 301 /preguntas-frecuentes https://www.altosdelpalmar.com/preguntas-frecuentes.html
Redirect 301 /constructora-alianza https://www.altosdelpalmar.com/constructora-alianza.html
Redirect 301 /contacto https://www.altosdelpalmar.com/contacto.html

Redirect 301 /terminos-condiciones https://www.altosdelpalmar.com/terminos-condiciones.html
Redirect 301 /gracias https://www.altosdelpalmar.com/gracias.html
Redirect 301 /error-404 https://www.altosdelpalmar.com/error-404.html

Redirect 301 /formulario-accion-mobile https://www.altosdelpalmar.com/formulario-accion-mobile.html

ErrorDocument 404 https://www.altosdelpalmar.com/error-404.html

# GZIP compression for text files: HTML, CSS, JS, Text, XML, fonts
# -----------------------------------------------------------------------
# Defining MIME types to ensure the web server actually knows about them.
# -----------------------------------------------------------------------
<IfModule mod_mime.c>
    AddType application/javascript          js
    AddType application/vnd.ms-fontobject   eot
    AddType application/x-font-ttf          ttf ttc
    AddType font/opentype                   otf
    AddType application/x-font-woff         woff
    AddType image/svg+xml                   svg svgz 
    AddEncoding gzip                        svgz
</Ifmodule>

# -----------------------------------------------------------------------
# Compressing output.
# -----------------------------------------------------------------------
<IfModule mod_deflate.c>
    AddOutputFilterByType DEFLATE text/html text/plain text/css application/json
    AddOutputFilterByType DEFLATE application/javascript
    AddOutputFilterByType DEFLATE text/xml application/xml text/x-component
    AddOutputFilterByType DEFLATE application/xhtml+xml application/rss+xml application/atom+xml
    AddOutputFilterByType DEFLATE image/x-icon image/svg+xml application/vnd.ms-fontobject application/x-font-ttf font/opentype
</Ifmodule>

# 1 YEAR
ExpiresActive On
<FilesMatch "\.(ttf|svg|woff|woff2|otf|ico|pdf|flv)$">
Header set Cache-Control "max-age=29030400, public"
ExpiresDefault "access plus 1 years"
Header unset Last-Modified
Header unset ETag
SetOutputFilter DEFLATE
</FilesMatch>

# 1 MONTHS
<FilesMatch "\.(jpg|jpeg|png|gif|swf|webp)$">
Header set Cache-Control "max-age=29030400, public"
ExpiresDefault "access plus 1 years"
SetOutputFilter DEFLATE
</FilesMatch>

<FilesMatch "\.(xml|txt|css|js)$">
Header set Cache-Control "max-age=29030400, public"
ExpiresDefault "access plus 1 years"
SetOutputFilter DEFLATE
</FilesMatch>

# 30 MIN
<FilesMatch "\.(html|htm|php)$">
Header set Cache-Control "max-age=29030400, public"
ExpiresDefault "access plus 1 years"
SetOutputFilter DEFLATE
</FilesMatch>